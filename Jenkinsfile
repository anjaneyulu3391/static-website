pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        git(url: 'git@github.com:anjaneyulu3391/static-website.git', branch: 'develop')
      }
    }

    stage('build') {
      steps {
        sh 'ls -ltr'
      }
    }

    stage('upload') {
      steps {
        sh 'ls'
      }
    }

    stage('depoly') {
      steps {
        sh 'ls'
      }
    }

  }
}